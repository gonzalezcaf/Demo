package com.empresa.area.libranzasrg;

import java.lang.Number;

rule "R11"
	dialect "mvel"
	enabled false
	when
		emp : empresa( tipo == "PRIVADA" )
		cli : cliente( respuesta_cr in ( 0, 4, 7 ) )
